# Configurações da Aplicação
PROJECT_NAME=Sistema de Clínica Médica
VERSION=1.0.0
DESCRIPTION=API para gerenciamento de clínica médica
API_V1_STR=/api/v1

# Banco de Dados PostgreSQL
DATABASE_URL=postgresql://clinica_user:clinica_password@db:5432/clinica_medica

# Segurança JWT
SECRET_KEY=sua-chave-secreta-muito-segura-mude-em-producao-123456789
